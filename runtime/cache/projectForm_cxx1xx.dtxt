    var query212029;
    $scope.loadService212029 = function (query, skipTimeout) {
        if (query212029 != query) {
            query212029 = query;
            if (!!skipTimeout) {
                return service212029.list($scope,$dibContainer, query).then(function (items) {
                    $scope.dropdowns.service212029.items = items;
                });
            } else {
                clearTimeout(setTimeoutCancel212029);
                setTimeoutCancel212029 = setTimeout(function () { 
                    service212029.list($scope,$dibContainer, query).then(function (items) {
                        $scope.dropdowns.service212029.items = items;
                    });
                }, 100);
            }
        } 
    };
    $scope.dropdowns.service212029.searchTextChange = function (query) {
        $scope.loadService212029(query);
    };
    $scope.dropdowns.service212029.getItems = function (query,modelType) {
        return service212029.list($scope,$dibContainer, query,modelType);
    };
    $scope.select212029 = function (model,modelName,fieldName) {
        if (!!modelName == false) modelName ="model";
        var $element =  $('#ci212029');
        if (!!$element.attr('select-model') === false) { 
            $element = $element.find('[select-model]');
        }
        var modelName =  'selected___' + fieldName;
        $element.trigger('select');
        if (!!model[modelName]  === false) { 
            return  false;
        }
        angular.forEach($scope.dropdowns.service212029.items, function (item, index)   {
            if (item.id == model[modelName]['id']) {
                model[modelName]['id_display_value'] = item.id_display_value; 
                return false;
            }
        });
    };
