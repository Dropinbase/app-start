									<md-button ng-if="!item.leaf && !!item.children == false" class="md-icon-button  md-squared" ng-click="menus.service209302.update(item)">
										<md-icon md-svg-icon="/files/dropins/setNgMaterial/dibMenu/img/open.svg">
										</md-icon>
									</md-button>
									<md-button ng-if="!item.leaf && !!item.children" class="md-icon-button  md-squared" ng-click="item.children = null">
										<md-icon md-svg-icon="/files/dropins/setNgMaterial/dibMenu/img/close.svg">
