		<md-sidenav  aria-label="menu" can-be-moved=true item-alias="menu"    ng-show="!!view.menu == false || view.menu.visible"  dib-disable="{{view.menu.disabled}}" ng-class="view.menu.class"  ng-style="view.menu.style" can-have-children=true flex=none class="md-sidenav-left" md-is-locked-open="$mdMedia('gt-md')" md-whiteframe="4" style="    " id="ci209302" dib-side-bar-status md-is-open="sideNavs.menu" md-component-id="menu" id='ci209302' aria-label="menu" dib-container-name='aatest' dib-config="{ listeners : { } }" >
